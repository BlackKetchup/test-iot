// Main.xml
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/cityTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="도시명"
        android:textSize="48sp"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/temperatureTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="온도"
        android:textSize="24sp"
        android:layout_below="@id/cityTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/humidityTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="습도"
        android:textSize="24sp"
        android:layout_below="@id/temperatureTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/weatherConditionTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="날씨 상태"
        android:textSize="24sp"
        android:layout_below="@id/humidityTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/windSpeedTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="풍속"
        android:textSize="24sp"
        android:layout_below="@id/weatherConditionTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>

    <Button
        android:id="@+id/seoulButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="서울"
        android:layout_below="@id/windSpeedTextView"
        android:layout_marginTop="40dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/incheonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="인천"
        android:layout_below="@id/windSpeedTextView"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@id/seoulButton"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/daejeonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="대전"
        android:layout_below="@id/windSpeedTextView"
        android:layout_marginTop="40dp"
        android:layout_toEndOf="@id/incheonButton"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/daeguButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="대구"
        android:layout_below="@id/seoulButton"
        android:layout_marginTop="30dp"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/ulsanButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="울산"
        android:layout_below="@id/incheonButton"
        android:layout_marginTop="30dp"
        android:layout_toEndOf="@id/daeguButton"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/busanButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="부산"
        android:layout_below="@id/daejeonButton"
        android:layout_marginTop="30dp"
        android:layout_toEndOf="@id/ulsanButton"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/gwangjuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="광주"
        android:layout_below="@id/daeguButton"
        android:layout_marginTop="30dp"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/sejongButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="세종"
        android:layout_below="@id/ulsanButton"
        android:layout_marginTop="30dp"
        android:layout_toEndOf="@id/gwangjuButton"
        android:layout_marginStart="20dp"/>

    <Button
        android:id="@+id/jejuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제주"
        android:layout_below="@id/busanButton"
        android:layout_marginTop="30dp"
        android:layout_toEndOf="@id/sejongButton"
        android:layout_marginStart="20dp"/>
</RelativeLayout>

