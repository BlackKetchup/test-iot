// Main.java
package com.example.myfirstproject01;

import android.content.Intent;
import android.os.Bundle;
import android.view.View;
import android.widget.Button;
import android.widget.TextView;
import androidx.appcompat.app.AppCompatActivity;

public class MainActivity extends AppCompatActivity {

    private TextView cityTextView;
    private TextView temperatureTextView;
    private TextView humidityTextView;
    private TextView weatherConditionTextView;
    private TextView windSpeedTextView;
    private Button seoulButton;
    private Button incheonButton;
    private Button daejeonButton;
    private Button daeguButton;
    private Button ulsanButton;
    private Button busanButton;
    private Button gwangjuButton;
    private Button sejongButton;
    private Button jejuButton;

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_main);

        cityTextView = findViewById(R.id.cityTextView);
        temperatureTextView = findViewById(R.id.temperatureTextView);
        humidityTextView = findViewById(R.id.humidityTextView);
        weatherConditionTextView = findViewById(R.id.weatherConditionTextView);
        windSpeedTextView = findViewById(R.id.windSpeedTextView);
        seoulButton = findViewById(R.id.seoulButton);
        incheonButton = findViewById(R.id.incheonButton);
        daejeonButton = findViewById(R.id.daejeonButton);
        daeguButton = findViewById(R.id.daeguButton);
        ulsanButton = findViewById(R.id.ulsanButton);
        busanButton = findViewById(R.id.busanButton);
        gwangjuButton = findViewById(R.id.gwangjuButton);
        sejongButton = findViewById(R.id.sejongButton);
        jejuButton = findViewById(R.id.jejuButton);

        seoulButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v){
                Intent intent = new Intent(MainActivity.this, Seoul.class);
                startActivity(intent);
            }
        });

        incheonButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Incheon.class);
                startActivity(intent);
            }
        });

        daejeonButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Deajeon.class);
                startActivity(intent);
            }
        });

        daeguButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Deagu.class);
                startActivity(intent);
            }
        });

        ulsanButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Ulsan.class);
                startActivity(intent);
            }
        });

        busanButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Busan.class);
                startActivity(intent);
            }
        });

        gwangjuButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Gwangju.class);
                startActivity(intent);
            }
        });

        sejongButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Sejong.class);
                startActivity(intent);
            }
        });

        jejuButton.setOnClickListener(new View.OnClickListener() {
            @Override
            public void onClick(View v) {
                Intent intent = new Intent(MainActivity.this, Jeju.class);
                startActivity(intent);
            }
        });
    }

    private void updateWeather(String city, String temperature, String humidity, String weatherCondition, String windSpeed) {
        cityTextView.setText(city);
        temperatureTextView.setText(temperature);
        humidityTextView.setText(humidity);
        weatherConditionTextView.setText(weatherCondition);
        windSpeedTextView.setText(windSpeed);
    }
}

